{
  "name": "@void-aurora/xml",
  "version": "0.1.0",
  "description": "A simple xml library based on htmlparser2.",
  "keywords": [
    "xml",
    "Void Aurora"
  ],
  "author": "duduluu <duduluu@outlook.com>",
  "license": "MIT",
  "repository": "github:void-aurora/utilities",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/xml.cjs.js",
  "module": "./dist/xml.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/xml.cjs.js",
      "import": "./dist/xml.esm.js"
    }
  },
  "globalName": "VoidAuroraUtils",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm run build",
    "test": "pnpm run ts -- scripts/test",
    "ts": "ts-node --project ../../tsconfig.dev.json"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "fs-extra": "^10.0.0",
    "vite": "^2.3.3"
  },
  "dependencies": {
    "dom-serializer": "^1.3.2",
    "domhandler": "^4.2.0",
    "domutils": "^2.6.0",
    "htmlparser2": "^6.1.0"
  }
}
