{
  "name": "@void-aurora/utils",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.1.0",
  "description": "Shared Utilities Module for Void Aurora.",
  "keywords": [
    "typescript",
    "javascript",
    "Void Aurora"
  ],
  "author": "duduluu <duduluu@outlook.com>",
  "license": "MIT",
  "repository": "github:void-aurora/utilities",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/utils.cjs.js",
  "module": "./dist/utils.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/utils.cjs.js",
      "import": "./dist/utils.esm.js"
    }
  },
  "sideEffects": false,
  "globalName": "VoidAuroraUtils",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "jest",
    "test:updateSnapshot": "jest --updateSnapshot",
    "prepare": "pnpm run build",
    "ts-node": "ts-node --project ../../tsconfig.dev.json"
  },
  "dependencies": {
    "type-fest": "^1.1.3"
  }
}
